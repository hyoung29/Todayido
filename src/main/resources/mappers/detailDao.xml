<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.metamong.todayido.dao.detailDao">

    <select id="seleteStore" resultType="storeDto" parameterType="Integer">
        SELECT * FROM store WHERE store_num=#{store_num}
    </select>
    <select id="seletereview" resultType="ReviewDto" parameterType="Integer">
        SELECT * FROM review WHERE store_num=#{store_num}
    </select>
</mapper>